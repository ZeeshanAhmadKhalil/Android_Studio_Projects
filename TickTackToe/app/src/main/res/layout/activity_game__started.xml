<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mk141.ticktacktoe.Game_Started">


    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick1"
        android:clickable="false"
        android:enabled="true"
        android:textSize="80dp" />

    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn2"
        android:layout_alignTop="@+id/btn1"
        android:layout_centerHorizontal="true"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick2"
        android:enabled="true"
        android:textSize="80dp" />

    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn3"
        android:layout_alignTop="@+id/btn2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick3"
        android:enabled="true"
        android:textSize="80dp" />
    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@id/btn1"
        android:layout_marginTop="10dp"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick4"
        android:textSize="80dp" />

    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn5"
        android:layout_alignTop="@+id/btn4"
        android:layout_centerHorizontal="true"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick5"
        android:textSize="80dp" />

    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn6"
        android:layout_alignTop="@+id/btn5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick6"
        android:textSize="80dp" />
    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@id/btn4"
        android:layout_marginTop="10dp"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick7"
        android:textSize="80dp" />

    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn8"
        android:layout_alignTop="@+id/btn7"
        android:layout_centerHorizontal="true"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick8"
        android:textSize="80dp" />

    <Button
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:id="@+id/btn9"
        android:layout_alignTop="@+id/btn8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:nestedScrollingEnabled="false"
        android:onClick="onClick9"
        android:textSize="80dp" />


</RelativeLayout>
